#include <iostream>
//user namespace std;

int main(){
  //Input a number to / by 2 
  int counter=5;

  while(counter <= 10){
    //IF the counter is /2 and equal to 0 print counter
    if(counter/2 == 0)
      std:: cout<< counter << std::endl;
    counter++;  // +incement counter
  }
  return 0;
};
